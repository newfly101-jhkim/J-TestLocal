        <result property="lottoNo6"      column="lotto_no6" />
        <result property="lottoNo7Bonus"      column="lotto_no7_bonus" />
        <result property="result"      column="result" />
        <result property="totalSellAmount"      column="total_sell_amount" />
    </resultMap>

